document.removeEventListener(\"mousedown\", handleClick);\n    };\n  }, [showNav]);\n  const handleClick = (e) => {\n    if (node.current.contains(e.target)) {\n      return;\n    }\n    setShowNav(false);\n  };\n  const dispatch = useDispatch();\n  if (props.uuid) {\n    return (\n      <section>\n        <div className=\"nav--wrap\">\n          <div className=\"logo-section\">\n            <HeaderSVGLogo />\n            <HeaderSVGText />\n          </div>\n          <nav ref={node} className={`navigation ${className}`}>\n            <section\n              className=\"navigation__icon\"\n              onClick={() => setShowNav((showNav) => !showNav)}\n            >\n              <span className=\"topBar\"></span>\n              <span className=\"middleBar\"></span>\n              <span className=\"bottomBar\"></span>\n            </section>\n\n            <ul className=\"navigation__ul\">\n              {navlinks.map((link, index) => (\n                <li\n                  key={index}\n                  onClick={() => setShowNav((showNav) => !showNav)}\n                >\n                  <NavLink to={link.link}>{link.text}</NavLink>\n                </li>\n              ))}\n              <NavLink activeClassName=\"link--active\" to=\"/register\">\n                signIn\n              </NavLink>\n            </ul>\n          </nav>\n          <ul className=\"navigation--ul\">\n            {navlinks.map((link, ind) => (\n              <li key={ind}>\n                <NavLink activeClassName=\"link--active\" to={link.link}>\n                  {link.text}\n                </NavLink>\n              </li>\n            ))}\n            <li>\n              <NavLink onClick={() => dispatch(signOut())} to=\"/\">\n                signOut\n              </NavLink>\n            </li>\n          </ul>\n        </div>\n      </section>\n    );\n  }\n  return (\n    <section>\n      <div className=\"nav--wrap\">\n        <div className=\"logo-section\">\n          <HeaderSVGLogo />\n          <HeaderSVGText />\n        </div>\n        <nav ref={node} className={`navigation ${className}`}>\n          <section\n            className=\"navigation__icon\"\n            onClick={() => setShowNav((showNav) => !showNav)}\n          >\n            <span className=\"topBar\"></span>\n            <span className=\"middleBar\"></span>\n            <span className=\"bottomBar\"></span>\n          </section>\n\n          <ul className=\"navigation__ul\">\n            {navlinks.map((link, index) => (\n              <li key={index} onClick={() => setShowNav((showNav) => !showNav)}>\n                <NavLink to={link.link}>{link.text}</NavLink>\n              </li>\n            ))}\n            <NavLink activeClassName=\"link--active\" to=\"/register\">\n              signIn\n            </NavLink>\n          </ul>\n        </nav>\n        <ul className=\"navigation--ul\">\n          {navlinks.map((link, ind) => (\n            <li key={ind}>\n              <NavLink activeClassName=\"link--active\" to={link.link}>\n                {link.text}\n              </NavLink>\n            </li>\n          ))}\n          <li>\n            <NavLink activeClassName=\"link--active\" to=\"/register\">\n              signIn\n            </NavLink>\n          </li>\n        </ul>\n      </div>\n    </section>\n  );\n};\n\nconst mapStateToProps = (state) => {\n  return {\n    uuid: state.firebase.auth.uid,\n  };\n};\n\nexport default connect(mapStateToProps)(Navigation);\n","/Users/winhtetaung/Dev/React-Projects/cat-website/src/assets/misc/page-render.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/pages/error.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/pages/breeds.js",["102","103"],"import React, { useEffect, useState } from \"react\";\nimport \"react-lazy-load-image-component/src/effects/blur.css\";\nimport { FaHeart } from \"react-icons/fa\";\nimport { InfoStats, ProgressBars } from \"../components/InfoStats\";\nimport { Image } from \"../components/Catalouge\";\nimport FullScreenSpinner from \"../components/fullPageSpinner\";\nimport { useDispatch, connect } from \"react-redux\";\nimport { useFirestoreConnect, isLoaded } from \"react-redux-firebase\";\nimport { getCats } from \"../redux/actions\";\nimport { addBookmarks } from \"../redux/actions/bookmark\";\nimport IosArrowBack from \"react-ionicons/lib/IosArrowBack\";\nimport IosArrowForward from \"react-ionicons/lib/IosArrowForward\";\n\nfunction Breeds(props) {\n  const isLoaded = props.loading;\n  const catFacts = props.cats;\n  const [page, setPage] = useState(0);\n  const dispatch = useDispatch();\n\n  const addToBookmarks = (cat) => {\n    dispatch(addBookmarks(cat));\n  };\n\n  const prevPage = () => {\n    if (page !== 0) {\n      setPage((page) => page - 1);\n    }\n  };\n  const nextPage = () => {\n    setPage((page) => page + 1);\n  };\n\n  useEffect(() => {\n    dispatch(getCats(page));\n  }, [page, dispatch]);\n\n  return isLoaded ? (\n    <FullScreenSpinner />\n  ) : (\n    <section className=\"breeds\">\n      {catFacts.map((cat, ind) => (\n        <div className=\"breed-info-wrapper\" key={ind}>\n          <div className=\"info--image\">\n            <Image cat={cat.image} name={cat.name} />\n            <button onClick={() => addToBookmarks(cat)}>\n              <FaHeart className=\"icon\" size={50} />\n            </button>\n          </div>\n          <div className=\"info--stats\">\n            <InfoStats\n              catName={cat.name}\n              origin={cat.origin}\n              description={cat.description}\n              wiki_url={cat.wikipedia_url}\n              cfa_url={cat.cfa_url}\n            />\n            <ProgressBars\n              adapt={cat.adaptability}\n              affection={cat.affection_level}\n              friendlyness={cat.stranger_friendly}\n              intelligence={cat.intelligence}\n              health_issues={cat.health_issues}\n              rare={cat.rare}\n            />\n          </div>\n        </div>\n      ))}\n\n      <div className=\"pagination\">\n        {page === 0 ? (\n          <button\n            disabled={true}\n            style={{ opacity: \"0\" }}\n            onClick={() => prevPage()}\n          >\n            <IosArrowBack />\n          </button>\n        ) : (\n          <button onClick={() => prevPage()}>\n            <IosArrowBack />\n          </button>\n        )}\n        <button>\n          <span className=\"indicator\">{page + 1}</span>\n        </button>\n        {page > 5 ? (\n          <button\n            style={{ opacity: \"0\" }}\n            disabled={true}\n            onClick={() => nextPage()}\n          >\n            <IosArrowForward />\n          </button>\n        ) : (\n          <button onClick={() => nextPage()}>\n            <IosArrowForward />\n          </button>\n        )}\n      </div>\n    </section>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    loading: state.cats.loading,\n    cats: state.cats.cats,\n    uid: state.firebase.auth.uid,\n  };\n};\n\nexport default connect(mapStateToProps)(Breeds);\n","/Users/winhtetaung/Dev/React-Projects/cat-website/src/pages/gallery.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/components/InfoStats.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/components/Catalouge.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/components/fullPageSpinner.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/redux/store.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/redux/reducers/index.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/redux/reducers/catReducer.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/redux/actions/index.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/config/firestoreConfig.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/src/redux/reducers/authReducer.js",[],"/Users/winhtetaung/Dev/React-Projects/cat-website/